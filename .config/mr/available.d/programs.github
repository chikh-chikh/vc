[${XDG_HOME:-$HOME}/DICOM/OHIF]
checkout = 
    git clone 'git@github.com:chikh-chikh/md_watcher.git' 'OHIF'
    cd OHIF
    git remote add upstream 'git@github.com:OHIF/Viewers.git'
upgrade =
    git pull upstream
    git merge upstream/master user
    git push origin user

